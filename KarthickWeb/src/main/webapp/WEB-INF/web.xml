<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<display-name>Login</display-name>
  	<description></description>
  	<servlet-class>crud.basic.Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HandleCreate</servlet-name>
  	<display-name>HandleCreate</display-name>
  	<description></description>
  	<servlet-class>crud.basic.HandleCreate</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HandleList</servlet-name>
  	<display-name>HandleList</display-name>
  	<description></description>
  	<servlet-class>crud.basic.HandleList</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HandleEdit</servlet-name>
  	<display-name>HandleEdit</display-name>
  	<description></description>
  	<servlet-class>crud.basic.HandleEdit</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HandleUpdate</servlet-name>
  	<display-name>HandleUpdate</display-name>
  	<description></description>
  	<servlet-class>crud.basic.HandleUpdate</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HandleDelete</servlet-name>
  	<display-name>HandleDelete</display-name>
  	<description></description>
  	<servlet-class>crud.basic.HandleDelete</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HandleFind</servlet-name>
  	<display-name>HandleFind</display-name>
  	<description></description>
  	<servlet-class>crud.basic.HandleFind</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HandleLogout</servlet-name>
  	<display-name>HandleLogout</display-name>
  	<description></description>
  	<servlet-class>crud.basic.HandleLogout</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/log</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HandleCreate</servlet-name>
  	<url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HandleList</servlet-name>
  	<url-pattern>/view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HandleEdit</servlet-name>
  	<url-pattern>/edit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HandleUpdate</servlet-name>
  	<url-pattern>/update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HandleDelete</servlet-name>
  	<url-pattern>/erase</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HandleFind</servlet-name>
  	<url-pattern>/find</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HandleLogout</servlet-name>
  	<url-pattern>/away</url-pattern>
  </servlet-mapping>
  
</web-app>
